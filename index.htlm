<!DOCTYPE html>
<html>
<head>
  <title>Andrews Data</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      background-color: #f5f5f5;
      margin: 0;
      padding: 20px;
    }

    h1 {
      color: #333;
    }

    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 20px;
      background-color: #fff;
      box-shadow: 0px 2px 6px rgba(0, 0, 0, 0.3);
    }

    th, td {
      padding: 12px 15px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }

    th {
      background-color: #f9f9f9;
      font-weight: bold;
    }
  </style>
  <script src="https://sheets.googleapis.com/1YpDfRnODS__5QI3NUUNt_TMVP-1unSvaEIOd2nVvLJM/client.js"></script>
  <script>
    function getData() {
      var spreadsheetID = "1YpDfRnODS__5QI3NUUNt_TMVP-1unSvaEIOd2nVvLJM";
      var sheetID = "1887165909";
      var url = "https://spreadsheets.google.com/feeds/list/" + spreadsheetID + "/" + sheetID + "/public/values?alt=json";

      fetch(url)
        .then(response => response.json())
        .then(data => {
          var entries = data.feed.entry;
          var table = document.getElementById("data");

          entries.forEach(entry => {
            var row = document.createElement("tr");
            var column1 = entry.gsx$column1.$t;
            var column2 = entry.gsx$column2.$t;
            var column3 = entry.gsx$column2.$t;
            var column4 = entry.gsx$column2.$t;
            // Repeat the above line for each column you want to display

            var cell1 = document.createElement("td");
            cell1.textContent = column1;
            row.appendChild(cell1);

            var cell2 = document.createElement("td");
            cell2.textContent = column2;
            row.appendChild(cell2);

            var cell3 = document.createElement("td");
            cell3.textContent = column2;
            row.appendChild(cell3);

            var cell4 = document.createElement("td");
            cell4.textContent = column2;
            row.appendChild(cell4);

            var cell5 = document.createElement("td");
            cell5.textContent = column2;
            row.appendChild(cell5);
            // Repeat the above lines for each column you want to display

            table.appendChild(row);
          });
        });
    }
  </script>
</head>
<body>
  <h1>Data from Google Sheets</h1>
  <button onclick="getData()">Load Data</button>
  <table id="data">
    <tr>
      <th>What's your name? </th>
      <th>Whats your email address? </th>
      <th>What's your address?</th>
      <th>What's your phone number?</th>
      <th>Can you work full time?</th>
      <!-- Add additional th elements for each column you want to display -->
    </tr>
  </table>
</body>
</html>
